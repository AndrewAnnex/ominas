;===========================================================================
; cam_set_fn_data_p
;
;
;===========================================================================
pro cam_set_fn_data_p, cxp, fn_data_p
@nv_lib.include
 cdp = class_extract(cxp, 'CAMERA')
 cd = nv_dereference(cdp)

 cd.fn_data_p=fn_data_p

 nv_rereference, cdp, cd
 nv_notify, cdp, type = 0
end
;===========================================================================



