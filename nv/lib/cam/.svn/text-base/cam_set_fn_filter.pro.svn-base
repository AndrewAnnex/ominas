;===========================================================================
; cam_set_fn_filter
;
;
;===========================================================================
pro cam_set_fn_filter, cxp, fn_filter
@nv_lib.include
 cdp = class_extract(cxp, 'CAMERA')
 cd = nv_dereference(cdp)

 cd.fn_filter=fn_filter

 nv_rereference, cdp, cd
 nv_notify, cdp, type = 0
end
;===========================================================================



