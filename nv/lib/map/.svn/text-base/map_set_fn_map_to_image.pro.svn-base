pro map_set_fn_map_to_image, mdp, fn
@nv_lib.include
 md = nv_dereference(mdp)

 md.fn_map_to_image=fn

 nv_rereference, mdp, md
 nv_notify, mdp, type = 0
end
;===========================================================================
