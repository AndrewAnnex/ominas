;===========================================================================
; map_set_radii
;
;===========================================================================
pro map_set_radii, mdp, radii
@nv_lib.include
 md = nv_dereference(mdp)

 md.radii=radii

 nv_rereference, mdp, md
 nv_notify, mdp, type = 0
end
;===========================================================================
