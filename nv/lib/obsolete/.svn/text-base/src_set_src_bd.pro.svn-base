;===========================================================================
; src_set_src_bd
;
;
;===========================================================================
pro src_set_src_bd, scdp, bdp
 scd = nv_dereference(scdp)

 scd.src_bd = bdp

 nv_rereference, scdp, scd
 nv_notify, scdp, type = 0
end
;===========================================================================



