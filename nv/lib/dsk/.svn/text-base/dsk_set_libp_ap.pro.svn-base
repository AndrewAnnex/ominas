;===========================================================================
; dsk_set_libp_ap
;
;
;===========================================================================
pro dsk_set_libp_ap, dkx, libp_ap, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_set_libp_ap', 'frame_bd required.'

 orb_set_libp_ap, dkd, frame_bd, libp_ap

end
;===========================================================================
