;===========================================================================
; dsk_get_libp_ap
;
;
;===========================================================================
function dsk_get_libp_ap, dkx, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_get_libp_ap', 'frame_bd required.'

 libp_ap = orb_get_libp_ap(dkd, frame_bd)

 return, libp_ap
end
;===========================================================================



