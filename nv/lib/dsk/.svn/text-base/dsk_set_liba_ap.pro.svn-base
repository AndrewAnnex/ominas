;===========================================================================
; dsk_set_liba_ap
;
;
;===========================================================================
pro dsk_set_liba_ap, dkx, liba_ap, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_set_liba_ap', 'frame_bd required.'

 orb_set_liba_ap, dkd, frame_bd, liba_ap

end
;===========================================================================
