;===========================================================================
; dsk_get_liba_lan
;
;
;===========================================================================
function dsk_get_liba_lan, dkx, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_get_liba_lan', 'frame_bd required.'


 liba_lan = orb_get_liba_lan(dkd, frame_bd)

 return, liba_lan
end
;===========================================================================
