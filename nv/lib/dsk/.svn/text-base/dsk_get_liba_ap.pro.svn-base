;===========================================================================
; dsk_get_liba_ap
;
;
;===========================================================================
function dsk_get_liba_ap, dkx, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_get_liba_ap', 'frame_bd required.'

 liba_ap = orb_get_liba_ap(dkd, frame_bd)

 return, liba_ap
end
;===========================================================================



