;===========================================================================
; dsk_get_dlandt
;
;
;===========================================================================
function dsk_get_dlandt, dkx, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_get_dlandt', 'frame_bd required.'


 dlandt = orb_get_dlandt(dkd, frame_bd)

 return, dlandt
end
;===========================================================================
