;===========================================================================
; dsk_set_libt_ap
;
;
;===========================================================================
pro dsk_set_libt_ap, dkx, libt_ap, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_set_libt_ap', 'frame_bd required.'

 orb_set_libt_ap, dkd, frame_bd, libt_ap

end
;===========================================================================
