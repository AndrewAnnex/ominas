;===========================================================================
; dsk_set_ap
;
;
;===========================================================================
pro dsk_set_ap, dkx, ap, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_set_ap', 'frame_bd required.'

 orb_set_ap, dkd, frame_bd, ap

end
;===========================================================================
