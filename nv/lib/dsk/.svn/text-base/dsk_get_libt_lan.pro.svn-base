;===========================================================================
; dsk_get_libt_lan
;
;
;===========================================================================
function dsk_get_libt_lan, dkx, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_get_libt_lan', 'frame_bd required.'


 libt_lan = orb_get_libt_lan(dkd, frame_bd)

 return, libt_lan
end
;===========================================================================
