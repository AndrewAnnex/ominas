;===========================================================================
; dsk_set_liba_lan
;
;
;===========================================================================
pro dsk_set_liba_lan, dkx, liba_lan, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_set_liba_lan', 'frame_bd required.'

 orb_set_liba_lan, dkd, frame_bd, liba_lan

end
;===========================================================================
