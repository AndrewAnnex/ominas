;===========================================================================
; dsk_set_libt_lan
;
;
;===========================================================================
pro dsk_set_libt_lan, dkx, libt_lan, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_set_libt_lan', 'frame_bd required.'

 orb_set_libt_lan, dkd, frame_bd, libt_lan

end
;===========================================================================
