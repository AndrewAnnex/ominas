;===========================================================================
; dsk_get_node
;
;  Returns inertial vectors.
;
;===========================================================================
function dsk_get_node, dkx, frame_bd
@nv_lib.include

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_get_node', 'frame_bd required.'

 node = orb_get_ascending_node(dkx, frame_bd)

 return, node
end
;===========================================================================



