;===========================================================================
; dsk_get_libt_ap
;
;
;===========================================================================
function dsk_get_libt_ap, dkx, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_get_libt_ap', 'frame_bd required.'

 libt_ap = orb_get_libt_ap(dkd, frame_bd)

 return, libt_ap
end
;===========================================================================



