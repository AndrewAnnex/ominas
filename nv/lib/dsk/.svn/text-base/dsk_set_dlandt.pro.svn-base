;===========================================================================
; dsk_set_dlandt
;
;
;===========================================================================
pro dsk_set_dlandt, dkx, dlandt, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_set_dlandt', 'frame_bd required.'

 orb_set_dlandt, dkd, frame_bd, dlandt

end
;===========================================================================
