;===========================================================================
; dsk_set_lan
;
;
;===========================================================================
pro dsk_set_lan, dkx, lan, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_set_lan', 'frame_bd required.'

 orb_set_lan, dkd, frame_bd, lan

end
;===========================================================================
