;===========================================================================
; dsk_set_dapdt
;
;
;===========================================================================
pro dsk_set_dapdt, dkx, dapdt, frame_bd
@nv_lib.include
 dkd = class_extract(dkx, 'DISK')

 if(NOT keyword_set(frame_bd)) then $
             nv_message, name='dsk_sdet_dapdt', 'frame_bd required.'

 orb_set_dapdt, dkd, frame_bd, dapdt

end
;===========================================================================
