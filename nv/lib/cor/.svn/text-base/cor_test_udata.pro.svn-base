;=============================================================================
; cor_test_udata
;
;=============================================================================
function cor_test_udata, crxp, name
@nv_lib.include
 crdp = class_extract(crxp, 'CORE')
 nv_notify, crdp, type = 1
 crd = nv_dereference(crdp)

 if(NOT ptr_valid(crd.udata_tlp)) then return, 0
 return, (tag_list_match(crd.udata_tlp, name) NE -1)[0]
end
;=============================================================================
