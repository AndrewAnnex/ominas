;===========================================================================
; cor_set_name
;
;
;===========================================================================
pro cor_set_name, crxp, name
@nv_lib.include
 crdp = class_extract(crxp, 'CORE')
 crd = nv_dereference(crdp)

 crd.name=name

 nv_rereference, crdp, crd
 nv_notify, crdp, type = 0
end
;===========================================================================



