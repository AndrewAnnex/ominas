;===========================================================================
; stn_set_primary
;
;
;===========================================================================
pro stn_set_primary, stxp, primary
@nv_lib.include
 stdp = class_extract(stxp, 'STATION')
 std = nv_dereference(stdp)

 std.primary=primary

 nv_rereference, stdp, std
 nv_notify, stdp, type = 0
end
;===========================================================================



