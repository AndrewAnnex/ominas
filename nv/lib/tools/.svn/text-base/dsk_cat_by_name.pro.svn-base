;===============================================================================
; dsk_cat_by_name
;
;
;===============================================================================
function dsk_cat_by_name, dkxs, names

 if(NOT keyword__set(dkxs)) then return, 0

 w = nwhere(cor_name(dkxs), names)
 if(w[0] EQ -1) then return, 0

 return, dsk_cat(dkxs[w])
end
;===============================================================================
