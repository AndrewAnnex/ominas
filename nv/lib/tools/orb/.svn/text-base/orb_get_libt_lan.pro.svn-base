;===========================================================================
; orb_get_libt_lan
;
;
;===========================================================================
function orb_get_libt_lan, xd, frame_bd

 nt = n_elements(xd)

 bd = class_extract(xd, 'BODY')

 libt = bod_libt(bd)

 libt_lan = reform(libt[1,*], nt, /over)

 return, libt_lan
end
;===========================================================================
