;===========================================================================
; orb_get_libt_ap
;
;
;===========================================================================
function orb_get_libt_ap, xd, frame_bd

 nt = n_elements(xd)

 bd = class_extract(xd, 'BODY')

 libt = bod_libt(bd)

 libt_ap = reform(libt[0,*], nt, /over) 

 return, libt_ap
end
;===========================================================================
