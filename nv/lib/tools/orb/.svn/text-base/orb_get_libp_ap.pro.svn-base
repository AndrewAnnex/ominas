;===========================================================================
; orb_get_libp_ap
;
;
;===========================================================================
function orb_get_libp_ap, xd, frame_bd

 nt = n_elements(xd)

 bd = class_extract(xd, 'BODY')

 libp = bod_libp(bd)

 libp_ap = reform(libp[0,*], nt, /over) 

 return, libp_ap
end
;===========================================================================
