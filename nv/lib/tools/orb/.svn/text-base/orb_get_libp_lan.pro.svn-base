;===========================================================================
; orb_get_libp_lan
;
;
;===========================================================================
function orb_get_libp_lan, xd, frame_bd

 nt = n_elements(xd)

 bd = class_extract(xd, 'BODY')

 libp = bod_libp(bd)

 libp_lan = reform(libp[1,*], nt, /over)

 return, libp_lan
end
;===========================================================================
