;====================================================================================
; nv_test_dd
;
;====================================================================================
function nv_test_dd, ddp
@nv.include
 if(NOT keyword_set(ddp)) then return, 0
 if(NOT ptr_valid(ddp)) then return, 0

 nv_notify, ddp, type = 1
 dd = nv_dereference(ddp)

 return, tag_exists(dd, 'DATA_DAP')
end
;====================================================================================
